<Application
    x:Class="PokeR.UWP.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PokeR.UWP">
    <Application.Resources>
        <x:String x:Key="AppName">Planning PokeR</x:String>
        <x:String x:Key="RoomID">Room ID</x:String>
        <Thickness x:Key="TextFieldSpacing">0,0,0,24</Thickness>
        <x:Double x:Key="ContainerMaxWidth">800</x:Double>
        <x:Double x:Key="Font-h2">32</x:Double>
        <x:Double x:Key="Font-small">12.8</x:Double>
        <Thickness x:Key="NoBorder">0,0,0,0</Thickness>
        <Thickness x:Key="Margin-Right-sm">0,0,8,0</Thickness>
        <Thickness x:Key="Padding-Pane">24,24,24,24</Thickness>
        <x:Double x:Key="LinkFieldHeight">32</x:Double>
        <x:Double x:Key="Font-h6">16</x:Double>
    </Application.Resources>

</Application>
